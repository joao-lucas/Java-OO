
public class Recipiente {
	double capacidade;
	double quantidade;

	public boolean adicionar(double qtd) {

		this.quantidade = quantidade + qtd;
		System.out.println("Quantidade: " + this.quantidade);
		return true;
	}

	public boolean remover(double qtd) {
		this.quantidade = quantidade - qtd;
		return true;
	}
}
